    LWV $0001
    STWR W0 ; W0 == $0001
    LWV string ; A = running ptr
    ; ----------- loop start
loop:
    STWR W1 ; W1 = running_ptr
    ARA
    LBI
    ARV exit
    JIF LZ
    ARWR W0 ; ADDR = $0001
    OUT
    LWR W1 ; A = running_ptr
    CXWX ; X = running_ptr
    LWR W0 ; A = 1
    ADD ; A = ++running ptr
    ARV loop
    JMP
exit:
    ZERO
    ARA
    OUT
string:
    XHL
    IN
    CPL
    LBR B0
