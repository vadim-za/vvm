    lwv $0001
    stwr w0 ; W0 == $0001
    lwv string ; A = running ptr
    ; ----------- loop start
loop:
    stwr w1 ; W1 = running_ptr
    ara
    lbi
    arv exit
    jif lz
    arwr w0 ; ADDR = $0001
    out
    lwr w1 ; A = running_ptr
    cxwx ; X = running_ptr
    lwr w0 ; A = 1
    add ; A = ++running ptr
    arv loop
    jmp
exit:
    zero
    ara
    out
string:
    .db 'S'
    .db 'T'
    .db 'R'
    .db 0
